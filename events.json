                "optional": false,
                "nullable": false
            },
            {
                "name": "member_count",
                "type": "integer",
                "description": "the approximate number of members in the thread, capped at 50",
                "optional": false,
                "nullable": false
            },
            {
                "name": "added_members*",
                "type": "array of thread member objects",
                "description": "the users who were added to the thread",
                "optional": true,
                "nullable": false
