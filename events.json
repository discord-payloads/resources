[
    {
        "name": "Ready",
        "payload": "None",
        "description": "The ready event is dispatched when a client has completed the initial handshake with the gateway (for new sessions). The ready event can be the largest and most complex event the gateway will send, as it contains all the state required for a client to begin interacting with the rest of the platform.",
        "fields": [
            {
                "name": "v",
                "type": "integer",
                "description": "gateway version",
                "optional": false,
                "nullable": false
            },
            {
                "name": "user",
                "type": "user object",
                "description": "information about the user including email",
                "optional": false,
                "nullable": false
            },
            {
                "name": "guilds",
                "type": "array of Unavailable Guild objects",
                "description": "the guilds the user is in",
                "optional": false,
                "nullable": false
            },
            {
                "name": "session_id",
                "type": "string",
                "description": "used for resuming connections",
                "optional": false,
                "nullable": false
            },
            {
                "name": "shard",
                "type": "array of two integers (shard_id, num_shards)",
                "description": "the shard information associated with this session, if sent when identifying",
                "optional": true,
                "nullable": false
            },
            {
                "name": "application",
                "type": "partial application object",
                "description": "contains id and flags",
                "optional": false,
                "nullable": false
            }
        ]
    },
    {
        "name": "Channel Pins Update",
